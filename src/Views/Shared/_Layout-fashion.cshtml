<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicons -->
    <link rel="icon" href="~/assets/img/favicons/favicon.ico">
    <!-- Libs CSS -->
    <link rel="stylesheet" href="~/assets/libs/flickity/dist/flickity.min.css">
    <link rel="stylesheet" href="~/assets/libs/%40fancyapps/fancybox/dist/jquery.fancybox.min.css">
    <link rel="stylesheet" href="~/assets/libs/%40fortawesome/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/assets/libs/simplebar/dist/simplebar.min.css">
    <link rel="stylesheet" href="~/assets/libs/highlightjs/styles/vs2015.css">
    <link rel="stylesheet" href="~/assets/libs/flickity-fade/flickity-fade.css">
    <link rel="stylesheet" href="~/assets/fonts/feather/feather.css">
    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/assets/css/theme.min.css">
    <title>BootShop - Bootstrap ecommerce theme Fashion</title>
</head>
<body>
    <!-- MODALS -->
    <!-- Newsletter: Horizontal -->
    @await Component.InvokeAsync("NewsletterHorizontal")

    <!-- Newsletter: Vertical -->

    @await Component.InvokeAsync("NewsletterVertical")
    <!-- Password Reset -->
    @await Component.InvokeAsync("PasswordReset")


    <!-- Product -->
    @await Component.InvokeAsync("Product")

    <!-- Search -->
    @await Component.InvokeAsync("Search")


    <!-- Shopping Cart -->
    @await Component.InvokeAsync("ShoppingCart")


    <!-- Sidebar -->
    @await Component.InvokeAsync("Sidebar")


    <!-- Size Chart -->
    @await Component.InvokeAsync("SizeChart")

    <!-- Wait List -->
    @await Component.InvokeAsync("WaitList")

    <!-- NAVBAR1 -->
    @await Component.InvokeAsync("NAVBAR1")


    @*<!-- NAVBAR2 -->
    @await Component.InvokeAsync("NAVBAR2")

    <!-- NAVBAR2 -->
    @await Component.InvokeAsync("NAVBAR3")*@
    @RenderBody()


    <!-- WELCOME -->
    @await Component.InvokeAsync("WELCOME")

    <!-- SALE -->
    @await Component.InvokeAsync("SALE")

    <!-- TOPSELLERS -->
    @await Component.InvokeAsync("TOPSELLERS")
    <!-- MUSTHAVES -->
    @await Component.InvokeAsync("MUSTHAVES")

    <!-- BRAND -->
    @await Component.InvokeAsync("BRAND")
    <!-- INSTASHOP -->
    @await Component.InvokeAsync("INSTASHOP")
    <!-- FEATURES -->
    @await Component.InvokeAsync("FEATURES")

    <!-- FOOTER -->
    @await Component.InvokeAsync("Footer1")

    <!-- JAVASCRIPT -->
    <!-- Libs JS -->
    <script src="~/assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/libs/flickity/dist/flickity.pkgd.min.js"></script>
    <script src="~/assets/libs/%40fancyapps/fancybox/dist/jquery.fancybox.min.js"></script>
    <script src="~/assets/libs/smooth-scroll/dist/smooth-scroll.min.js"></script>
    <script src="~/assets/libs/simplebar/dist/simplebar.min.js"></script>
    <script src="~/assets/libs/list.js/dist/list.min.js"></script>
    <script src="~/assets/libs/jarallax/dist/jarallax.min.js"></script>
    <script src="~/assets/libs/highlightjs/highlight.pack.min.js"></script>
    <script src="~/assets/libs/flickity-fade/flickity-fade.js"></script>
    <!-- Map (replace the API key to enable) -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCnKt8_N4-FKOnhI_pSaDL7g_g-XI1-R9E"></script>
    <!-- Theme JS -->
    <script src="~/assets/js/theme.min.js"></script>

</body>

</html>
